{
  "framework": "other",
  "installCommand": "pnpm i --no-frozen-lockfile",
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist/public",
  "env": {
    "ENABLE_EXPERIMENTAL_COREPACK": "1"
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
